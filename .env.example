# disk config
STORAGE_DIR=/home/me/databarn/data

# ClickHouse
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=changeme
CLICKHOUSE_DB=default
CLICKHOUSE_MAX_MEMORY_USAGE=10000000000
CLICKHOUSE_USE_UNCOMPRESSED_CACHE=1
CLICKHOUSE_MAX_THREADS=auto(4)
CLICKHOUSE_MAX_INSERT_THREADS=auto(2)
CLICKHOUSE_MAX_FINAL_THREADS=auto(4)
CLICKHOUSE_PARALLEL_VIEW_PROCESSING=1
CLICKHOUSE_USE_ASYNC_EXECUTOR_FOR_MATERIALIZED_VIEWS=1
CLICKHOUSE_MAX_BYTES_BEFORE_EXTERNAL_SORT=4294967296
CLICKHOUSE_MAX_BYTES_BEFORE_EXTERNAL_GROUP_BY=4294967296
CLICKHOUSE_UNCOMPRESSED_CACHE_SIZE=4294967296
CLICKHOUSE_MARK_CACHE_SIZE=2147483648


# api
API_CLICKHOUSE_HOST=localhost
API_CLICKHOUSE_USER=api
API_CLICKHOUSE_PORT=8123
API_CLICKHOUSE_PASSWORD=changeme
API_CLICKHOUSE_DB=analytics
API_RATE_LIMIT_PER_MINUTE=100
API_CACHE_TTL_SECONDS=3600
API_HOST=127.0.0.1
API_PORT=8080

# dbt
DBT_RUN_LOCKFILE=dbt/.dbt_run.lock
DBT_RUN_LOCK_TIMEOUT=30
DBT_PRINTER_WIDTH=120
DBT_LOG_FORMAT=text
DBT_USE_COLORS=true
DBT_CLICKHOUSE_HOST=localhost
DBT_CLICKHOUSE_USER=dbt
DBT_CLICKHOUSE_PASSWORD=changeme
DBT_CLICKHOUSE_STAGING_DB=dbt
DBT_CLICKHOUSE_DB=analytics
DBT_ENVIO_BALANCES_DB=envio

# dlt
DLT_ENV=development
DLT_CLICKHOUSE_HOST=localhost
DLT_CLICKHOUSE_PORT=9000
DLT_CLICKHOUSE_HTTP_PORT=8123
DLT_CLICKHOUSE_USER=dlt
DLT_CLICKHOUSE_PASSWORD=changeme
DLT_CLICKHOUSE_DB=dlt
DLT_PIPELINE_ITERATION_TIMEOUT=30 # seconds

# PostgreSQL - beefy-db source
BEEFY_DB_HOST=postgres.example.com
BEEFY_DB_PORT=5432
BEEFY_DB_NAME=beefy-db
BEEFY_DB_USER=postgres
BEEFY_DB_PASSWORD=changeme

# MinIO
MINIO_ENDPOINT=http://localhost:9002
MINIO_ACCESS_KEY=admin
MINIO_SECRET_KEY=changeme
MINIO_DLT_STAGING_BUCKET=dlt-staging

# Traefik
TRAEFIK_LOG_LEVEL=INFO
TRAEFIK_DOCKER_NETWORK=beefy-databarn_frontend_network # as seen in `docker network ls`, not the compose shorthand
TRAEFIK_EMAIL=admin@example.com

# Postgres
POSTGRES_ADMIN_USER=postgres
POSTGRES_ADMIN_PASSWORD=postgres
POSTGRES_ADMIN_DB=postgres

# Grafana
GRAFANA_ROOT_URL=http://localhost:3000
GRAFANA_WEBAPP_DOMAIN_NAME=localhost:3000
GRAFANA_LOG_LEVEL=debug
GRAFANA_SECRET_KEY=changeme
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=changeme
GRAFANA_POSTGRES_HOST=postgres
GRAFANA_POSTGRES_PORT=5432
GRAFANA_POSTGRES_USER=grafana
GRAFANA_POSTGRES_DB=grafana
GRAFANA_POSTGRES_PASSWORD=changeme
GRAFANA_CLICKHOUSE_USER=grafana
GRAFANA_CLICKHOUSE_HOST=clickhouse
GRAFANA_CLICKHOUSE_PORT=8123
GRAFANA_CLICKHOUSE_PASSWORD=changeme
GRAFANA_CLICKHOUSE_DB=analytics
GRAFANA_REDIS_CONNSTR=addr=redis:6379,pool_size=20,db=2,ssl=false
GRAFANA_REDIS_PREFIX=grafana:
# set callback url to https://${GRAFANA_WEBAPP_DOMAIN_NAME}/login/github
GRAFANA_GITHUB_CLIENT_ID=changeme
GRAFANA_GITHUB_CLIENT_SECRET=changeme
GRAFANA_GITHUB_ALLOWED_ORG=beefyfinance

# Superset
SUPERSET_ENV=development
SUPERSET_LOG_LEVEL=INFO
SUPERSET_POSTGRES_DB=superset
SUPERSET_POSTGRES_USER=superset
SUPERSET_POSTGRES_PASSWORD=changeme
SUPERSET_CLICKHOUSE_HOST=clickhouse
SUPERSET_CLICKHOUSE_PORT=8123
SUPERSET_CLICKHOUSE_USER=superset
SUPERSET_CLICKHOUSE_PASSWORD=changeme
SUPERSET_CLICKHOUSE_DB=analytics
SUPERSET_SECRET_KEY=changeme
SUPERSET_ADMIN_USER=admin
SUPERSET_ADMIN_PASSWORD=changeme
SUPERSET_ADMIN_EMAIL=admin@admin.com
SUPERSET_REDIS_CELERY_DB=0
SUPERSET_REDIS_RESULTS_DB=1
SUPERSET_WEBAPP_DOMAIN_NAME=localhost:8088
# set github app redirect url as https://${SUPERSET_WEBAPP_DOMAIN_NAME}/oauth-authorized/github
SUPERSET_GITHUB_CLIENT_ID=changeme
SUPERSET_GITHUB_CLIENT_SECRET=changeme
SUPERSET_GITHUB_ALLOWED_ORG=beefyfinance
SUPERSET_AUTH_USER_REGISTRATION_ROLE=Gamma

# Prometheus
PROMETHEUS_ENVIRONMENT=dev

# envio
ENVIO_CLICKHOUSE_USER=envio
ENVIO_CLICKHOUSE_PASSWORD=changeme
